( function ( $, window, undefined ) {
  $.extend( true, window, {
    "iui": {
      "util": {}
    }
  } );

  iui.util.List = function ( array ) {
    if ( !this instanceof iui.util.List ) {
      return new iui.util.List( array );
    }

    this.items = array ? $.isArray( array ) ? array : [ array ] : [];
  };

  $.extend( iui.util.List.prototype, {
    add: function () {},
    remove: function () {},
    has: function () {},
    first: function () {},
    last: function () {},
  } );

}( jQuery, window ) );
